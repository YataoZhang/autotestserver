{
  "name": "autotestserver",
  "version": "1.0.0",
  "description": "web端自动化回归测试后台服务",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@cypress/chrome-recorder": "^2.3.1",
    "@prisma/client": "5.11.0",
    "@puppeteer/replay": "^2.10.2",
    "cypress": "^13.2.0",
    "egg": "^3.17.5",
    "egg-scripts": "2",
    "egg-validate": "^2.0.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prisma": "^5.11.0",
    "uid": "^2.0.2"
  },
  "devDependencies": {
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "8",
    "eslint-config-egg": "13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "PORT=8801 egg-scripts start --daemon --title=egg-server-autoTestServer --stdout=logs/script/stdout.log --stderr=logs/script/stderr.log",
    "stop": "egg-scripts stop --title=egg-server-autoTestServer",
    "dev": "egg-bin dev --port=8801",
    "test": "npm run lint -- --fix && npm run test:local",
    "test:local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "zhangyatao",
  "license": "MIT"
}